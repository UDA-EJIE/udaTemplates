statics.path=/{{project_name}}Statics
xlnets.inPortal=FALSE
{% if xlnets %}
xlnets.path=https://xlnets.servicios.des.ejgv.euskalsarea.eus/n38a/N38LoginInicioServlet
isEjie=true
{% else %}
isEjie=false
{% endif %}
# Redirection on auth error
xhr.redirectOnError=true
xhr.unauthorizedPage=referer

#Cookie
cookie.rootPath=true
cookie.secure=false

#Locale
locale.allowInheritance=false

#The System logs parameters
log.path=C:/datos/{{project_name}}/log
log.level.salidaEstandar=DEBUG
log.level.udaTrazas=DEBUG
log.level.aplicTrazas=DEBUG
log.level.auditoriaAccesos=INFO
#By default, the trace system audit  dataBase is disabled. 
#In case, you want to activate the audit system you need change the level trace to info, debug or trace level (is recomended the info level)
log.level.auditoriaBBDD=INFO

# Nivel de traza de las operaciones SQL
log.level.auditoriaBBDD.springJDBC.query=INFO
# Nivel de traza de los parï¿½metros de las operaciones SQL
log.level.auditoriaBBDD.springJDBC.queryParams=INFO

# Show detailed errors on failure
error.detailed=false

#File upload path
fileUpload.path=C:/datos/{{project_name}}/tmp

#File repositories parameters
repository.file.path=C:/datos/{{project_name}}/jackrabbit

#IberDok!
iberdokFolder=C:/datos/{{project_name}}/iberdok