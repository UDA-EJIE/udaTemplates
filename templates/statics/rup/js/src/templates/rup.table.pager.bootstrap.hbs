
  <div id="pg_{{id}}_pager" class="ui-pager-control rup_table_pager" role="group">
    <div id="{{idPager}}_left" class="pager_left"></div>
    <div id="{{idPager}}_center" class="pager_center">
      <div id="first_{{idPager}}">
        <a href="javascript:void(0)" class="linkPaginacion">{{firstPageText}}</a>
      </div>
      <div id="prev_{{idPager}}">
        <a href="javascript:void(0)" class="linkPaginacion">{{prevPageText}}</a>
      </div>
      <div id="next_{{idPager}}">
        <a href="javascript:void(0)" class="linkPaginacion">{{nextPageText}}</a>
      </div>
      <div id="last_{{idPager}}">
        <a href="javascript:void(0)" class="linkPaginacion">{{lastPageText}}</a>
      </div>

    </div>
    <div id="{{table_pager}}_right" class="pager_right">
      <div class="ui-paging-info">{{resultText}}</div>
    </div>
  </div>
