<div id="pagination_{{datatableId}}" class="rup-table-detail-info">
  <i class="fa fa-list-alt" aria-hidden="true" title="{{resultNumText}}"></i>
  <span id="rup_table_selectedElements_{{datatableId}}" class="rup-table-detail-currentElement" >
    {{resultNumText}}
  </span>
</div>
<div id="pag_{{datatableId}}" class="rup-table-detail-navigation">
    <button type="button" id="first_{{datatableId}}" class="btn-sm btn-outline-dark" alt="{{labelFirst}}">{{labelFirst}}</button>
    <button type="button" id="back_{{datatableId}}" class="btn-sm btn-outline-dark" alt="{{labelPrev}}">{{labelPrev}}</button>
    <button type="button" id="forward_{{datatableId}}" class="btn-sm btn-outline-dark" alt="{{labelNext}}">{{labelNext}}</button>
    <button type="button" id="last_{{datatableId}}" class="btn-sm btn-outline-dark" alt="{{labelLast}}">{{labelLast}}</button>
  </div>
  <div class="rup-table-detail-separator separador"></div>
